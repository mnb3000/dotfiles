[default_session]
startup_command = "la"

[[session]]
name = "🏡 Home"
path = "~"
startup_command = "la"

[[session]]
name = "📥 Downloads"
path = "~/Downloads"
startup_command = "y"

[[session]]
name = "📋 Documents"
path = "~/Documents/"
startup_command = "y"

[[session]]
name = "⚙️  zsh config"
path = "~"
startup_command = "nvim .zshrc .zprofile .zshenv .zplugins .zaliases .zstyles "

[[session]]
name = "📟 tmux config"
path = "~/.config/tmux"
startup_command = "nvim tmux.conf"

[[session]]
name = "💾 neovim config"
path = "~/.config/nvim"
startup_command = "nvim -c ':Telescope find_files'"

[[session]]
name = "📁 config dir"
path = "~/.config"
startup_command = "nvim -c ':Neotree' -c ':Telescope oldfiles only_cwd=true'"
