[default_session]
startup_command = "la"

[[session]]
name = "🏡 Home"
path = "~"
startup_command = "la"

[[session]]
name = "📥 Downloads"
path = "~/Downloads"
startup_command = "y"

[[session]]
name = "📋 Documents"
path = "~/Documents/"
startup_command = "y"

[[session]]
name = "⚙️  zsh config"
path = "~"
startup_command = "nvim .zshrc .zprofile .zshenv .zplugins .zaliases .zstyles "

[[session]]
name = "📁 config dir"
path = "~/.config"
startup_command = "nvim -c ':Neotree' -c ':Telescope oldfiles only_cwd=true'"

[[session]]
name = "💽 Debian (non-root)"
path = "~"
startup_command = "debian"

[[session]]
name = "❗ Debian (ROOT)"
path = "~"
startup_command = "debian --root"

[[session]]
name = "💻 Macbook SSH"
path = "~"
startup_command = "ssha Misha@myk-personal.taile3ea9.ts.net"
